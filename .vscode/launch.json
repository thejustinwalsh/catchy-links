  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Debug",
      "skipFiles": [
        "<node_internals>/**",
      ],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "debug"],
      "port": 9229
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "skipFiles": ["<node_internals>/**"],
      "port": 9229
    }
  ]
}